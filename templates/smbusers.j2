# {{ ansible_managed }}
#
# This file allows you to map usernames from the clients to the server.
# Unix_name = SMB_name1 SMB_name2 ...
#
# See section 'username map' in the manual page of smb.conf for more
# information.

{% for unixname, smbname in samba_usernames_map | dictsort %}
{{ unixname }} = "{{  smbname if smbname is string else smbname | join('", "')}}"
{% endfor %}
